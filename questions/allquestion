qno3)

<!DOCTYPE html>
<html>
<head>
	<title>Division cal</title>
</head>
<body>
<script type="text/javascript">
	var percentage=prompt("Enter your percentage");
	if(percentage>=80){
		alert("You Passed with Distinction");
	}else if(percentage>=60){
		alert("You Passed with First Division");
	}else if(percentage>=50){
		alert("You Passed with second Division");
	}else if(percentage>=40){
		alert("You Passed with third Division");
	}else{
		alert("You are fail");
	}
</script>
</body>
</html>

qno1) 

 <!DOCTYPE html>
<html>
<head>
	<title>greatest among 3</title>
</head>
<body>
<script type="text/javascript">
	var a=prompt("Enter your first number");
	var b=prompt("enter your second number");
	var c=prompt("enter your third number");
	if(a>b&&a>c){
		alert("a is greatest");
	}else if(b>a&&b>c){
		alert("b is greatest");
	
	}else{
		alert("c is greatest");
	}
</script>
</body>
</html>
